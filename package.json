{
  "name": "seance",
  "private": true,
  "scripts": {
    "build": "pnpm build:core && pnpm build:js:native",
    "build:core": "pnpm --filter @channeling/seance build",
    "build:native": "pnpm --filter @channeling/seance-native build",
    "build:demo": "pnpm --filter demo build",
    "dev:demo": "pnpm --filter demo dev",
    "publish:core": "pnpm --filter @channeling/seance publish --access public",
    "publish:native": "pnpm --filter @channeling/seance-native publish --access public"
  }
}
